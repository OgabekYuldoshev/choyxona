{
	"name": "choyxona",
	"version": "1.0.5",
	"description": "Choyxona for uzbek developers !!!",
	"main": "dist/main.js",
	"type": "module",
	"bin": {
		"choyxona": "dist/main.js"
	},
	"engines": {
		"node": ">= 18"
	},
	"repository": {
		"directory": "https://github.com/OgabekYuldoshev/choyxona",
		"type": "git"
	},
	"bugs": {
		"url": "https://github.com/OgabekYuldoshev/choyxona/issues"
	},
	"files": ["dist", "package.json"],
	"scripts": {
		"dev": "tsx src/main.ts",
		"build": "tsup ./src/main.ts",
		"start": "node ./dist/main.js",
		"lint": "pnpm biome check --write ./src",
		"prepare": "husky"
	},
	"keywords": ["chat", "socket", "lan", "lanchat", "choyxona"],
	"author": "Ogabek Yuldoshev <https://github.com/OgabekYuldoshev>",
	"license": "ISC",
	"dependencies": {
		"chalk": "^5.4.1",
		"minimist": "^1.2.8",
		"ora": "^8.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/minimist": "^1.2.5",
		"@types/node": "^22.14.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.3"
	},
	"lint-staged": {
		"*": ["pnpm lint", "git add ."]
	},
	"tsup": {
		"target": "esnext",
		"format": ["esm"]
	}
}
